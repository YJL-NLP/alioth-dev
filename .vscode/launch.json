    "version": "0.2.0",
    "configurations": [
        {
            "name": "operator",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/aliothc",
            "args": ["--work", "./demo/demo-operator", ":", "demo-operator"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
